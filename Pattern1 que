public class A
{
    public static void main(String[] args)
    {
        for(int i=1;i<=5;i++)//ROW
        {
            for(int j=i;j<5;j++)//Space
            {   
                    System.out.print(" ");  
               
            }
            for(int k=1;k<=i;k++)//columb
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
