//Find greater number between infinity number by using array
import java.util.Scanner;
class  ABcd {
      public static void main(String[] args) {
        
       int a[]={12,2,3,34,32};
       int max=a[0];
       for(int i=1;i<a.length;i++)
       {
        if(a[i]>max)
        {
            max=a[i];
        }
        
       }
 System.out.println("graeter number is="+max);
    }
}
