import java.util.Scanner;
class factorial
{
    public static void main (String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("Enter number:");
        int num=s.nextInt();
        int fact=1;
        for(int i=num;i>=1;i--)
        {
               fact*=i;
        }
        System.out.println("Factorial of "+num+"is ="+fact);
    }
}
