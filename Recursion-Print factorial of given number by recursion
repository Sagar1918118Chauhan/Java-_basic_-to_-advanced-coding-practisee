 
class Recursion {
    public static void main(String[] args) {
       
       factorial(5,1);
    }
     public static  void factorial(int n,int fact)
 {    
     if(n==1  )
     {
         
        fact*=n;
        System.out.println(fact);
     }
       else
       {
           fact*=n;
          factorial(n-1,fact);
          
       }
         
     
     
 }
}
